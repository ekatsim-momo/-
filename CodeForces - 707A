#include <iostream>
#include <cstring>
#include <cstdlib>
#include <vector>
#include <cmath>
#include <algorithm>
#include <set>
#include <queue>
#include <string>
#include <map>
using namespace std;
char arr[10];
int main()
{
    int r,c;
    cin>>r>>c;
    for(int i=0;i<r*c;i++)
    {
        scanf("%s",arr);
        if(arr[0]!='W'&&arr[0]!='B'&&arr[0]!='G')
        {
            cout<<"#Color\n";
            return 0;
        }
    }
    cout<<"#Black&White\n";
    return 0;
}
