#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long n,a,b,d,c,x,y,x1,y1,x2,y2;
    cin>>n;
    while(n--)
    {
        cin>>a>>b>>c>>d>>x>>y>>x1>>y1>>x2>>y2;
        if(-a+b+x<x1||b-a+x>x2||-c+d+y<y1||d-c+y>y2)
            cout<<"NO\n";
        else if(x-x1==0&&x2-x==0&&a!=0&&b!=0)
        cout<<"NO\n";
          else if(y-y1==0&&y2-y==0&&c!=0&&d!=0)
        cout<<"NO\n";
        else
            cout<<"YES\n";
    }
    return 0;
}
