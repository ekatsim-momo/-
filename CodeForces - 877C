#include<bits/stdc++.h>
using namespace std;
int main()
{
  int n;
  cin>>n;
  cout<<n+n/2<<endl;
  for(int i=2;i<=n;i+=2)
  {
      if(i!=2)
        printf(" %d",i);
      else
        printf("2");
  }
  for(int i=1;i<=n;i+=2)
        printf(" %d",i);
         for(int i=2;i<=n;i+=2)
        printf(" %d",i);
    return 0;
}
