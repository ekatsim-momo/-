#include<iostream>
#include<algorithm>
#include<cstdio>
#include<cstring>
#include<string>
#include<queue>
using namespace std;
int main()
{
	long long n=10, k=5, t;
	cin >> n >> k >> t;
		if (t <= k&&t<=n)
			cout << t << endl;
		else if (t <= n)
			cout << k << endl;
		else if (k - t + n >= 0)
			cout << (k - t + n) << endl;
		else
			cout << 0 << endl;
	return 0;
}
